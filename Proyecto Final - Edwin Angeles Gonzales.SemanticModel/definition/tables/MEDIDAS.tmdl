	measure Dif_%_Valor_total_inventario-SOFTWARE =
			
			VAR __BASELINE_VALUE =
				CALCULATE(
					[Valor_total_de_inventario],
					'BIENES_PATRIMONIALES'[CATEGORIA] IN { "SOFTWARE" }
				)
			VAR __MEASURE_VALUE = [Valor_total_de_inventario]
			RETURN
				IF(
					NOT ISBLANK(__MEASURE_VALUE),
					DIVIDE(__MEASURE_VALUE - __BASELINE_VALUE, __BASELINE_VALUE)
				)
